<% @notes.each do |note| %>
  <% if note.users.count > 1 %>
    <%= turbo_stream_from dom_id(note) %>
  <% end %>
<% end %>

<h1>Notes</h1>

<%= link_to "Home", root_path %>
<%= link_to "New note", new_note_path %>

<div id="notes">
  <% @notes.each do |note| %>
    <%= render note %>
    <%= link_to "Show this note", note %>
    <hr/>
  <% end %>
</div>
