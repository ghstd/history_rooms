<h1>Quiz_games</h1>

<%= link_to "Home", root_path %>

<%= button_to "Create a new quiz_game", quiz_games_path %>

<div id="quiz_games">
  <% @quiz_games.each do |quiz_game| %>
    <%= render quiz_game %>
    <%= link_to "Show this quiz_game", edit_quiz_game_path(quiz_game) %>
    <hr/>
  <% end %>
</div>
