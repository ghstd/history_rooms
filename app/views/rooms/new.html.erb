<div class="main">
	<div>
		Chat
	</div>
	<div>
		Game
	</div>
	<div>
		<h1>Home page</h1>
		<div><%= button_to "Log out", logout_path, method: :delete %></div>

		<div>
			<%= form_for :room, url: rooms_path do |f| %>
				<div>
					<input type="radio" id="model_1" name="model" value="StandardQuestion" checked />
					<label for="model_1">StandardQuestion</label>
					<br>
					<input type="radio" id="model_2" name="model" value="CoralQuestion" />
					<label for="model_2">CoralQuestion</label>
					<br>
					<input type="radio" id="model_3" name="model" value="DetailedQuestion" />
					<label for="model_3">DetailedQuestion</label>
					<br>
					<input type="radio" id="model_4" name="model" value="RandomQuestion" />
					<label for="model_4">RandomQuestion</label>
					<br>
					<input type="radio" id="model_5" name="model" value="WhatNextQuestion" />
					<label for="model_5">WhatNextQuestion</label>
					
					<br>
					<input type="radio" id="model_6" name="model" value="ArtQuestion" />
					<label for="model_6">WhatNextQuestion</label>
					<br>
					<input type="radio" id="model_7" name="model" value="CitiesQuestion" />
					<label for="model_7">WhatNextQuestion</label>
					<br>
					<input type="radio" id="model_8" name="model" value="IsTrueQuestion" />
					<label for="model_8">WhatNextQuestion</label>
					<br>
					<input type="radio" id="model_9" name="model" value="RiversQuestion" />
					<label for="model_9">WhatNextQuestion</label>
					<br>
					<input type="radio" id="model_10" name="model" value="SurvivalQuestion" />
					<label for="model_10">WhatNextQuestion</label>
					<br>
					<input type="radio" id="model_11" name="model" value="WhatMeanQuestion" />
					<label for="model_11">WhatNextQuestion</label>
					<br>
					<input type="radio" id="model_12" name="model" value="WhenWhereQuestion" />
					<label for="model_12">WhatNextQuestion</label>
					<br>
					<input type="radio" id="model_13" name="model" value="WhoSaidQuestion" />
					<label for="model_13">WhatNextQuestion</label>
				</div>
				<%= f.submit 'New Room' %>
			<% end %>
			<hr>
		</div>

		<div>rooms:</div>
			<% @rooms.each do |room| %>
				<div>room: 
				<%= link_to room.room_id, url_for(controller: 'rooms', action: 'show', id: room.room_id) %>
				<%= button_to "delete", url_for(controller: 'rooms', action: 'destroy', id: room.room_id), method: :delete %>
				</div>
			<% end %>
		</div>
	</div>
</div>
